						<control type="image">
							<posx>472</posx>
							<width>90</width>
							<height>90</height>
							<include>OverlayVisibility0</include>
						</control>
						<control type="image">
							<width>562</width>
							<height>104</height>
							<texture background="true">thumbs/banner_shadow_in.png</texture>
							<animation effect="fade" time="120" delay="120" start="100" end="0">Focus</animation>
						</control>
					</control>
				</focusedlayout>
			</control>
		</control>
	</include>
</includes>
