						<height>104</height>
						<align>center</align>
						<label>$INFO[ListItem.Label]</label>
						<shadowcolor>black</shadowcolor>
						<font>Font_Bold40</font>
						<visible>IsEmpty(ListItem.Art(banner)) + IsEmpty(ListItem.Art(tvshow.banner)) + IsEmpty(ListItem.Art(clearlogo))</visible>
					</control>
					<control type="image">
